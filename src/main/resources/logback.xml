<?xml version="1.0" encoding="UTF-8"?>

<configuration>
    <logger name="qlfight" level="ALL">
        <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
            <encoder>
                <pattern>
                    %d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%n
                </pattern>
            </encoder>
        </appender>
    </logger>

    <root level="ALL">
        <appender name="noop" class="ch.qos.logback.core.helpers.NOPAppender"></appender>
    </root>
</configuration>